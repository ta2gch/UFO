#!/bin/sh
#|-*- mode:lisp -*-|#
#|
exec ros -L sbcl-bin -Q -- $0 "$@"
|#

;(load (merge-pathnames "ufo.asd" *load-pathname*))
(ql:quickload :ufo :silent t)
(defun main (&rest argv)
  (declare (ignorable argv))
  (cond ((string= "install" (first argv))
	 (ufo:ufo :install (second argv)))
	((string= "remove" (first argv))
	 (ufo:ufo :remove (second argv)))
	((string= "update" (first argv))
	 (ufo:ufo :update (second argv)))))
	
